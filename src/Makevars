PKG_CPPFLAGS = -I./lib/
PKG_CFLAGS = $(C_VISIBILITY)

lib-files = \
        lib/rray.h \
        lib/library.c

export-files = \
        export/exported.c \
        export/init.c

all: $(SHLIB)

$(SHLIB): lib.o export.o

lib.c: $(lib-files)
	touch lib.c
export.c: $(export-files)
	touch export.c

.PHONY: all
